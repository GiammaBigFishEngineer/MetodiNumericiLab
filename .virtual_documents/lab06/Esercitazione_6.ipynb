alfa1 = 0
alfa2 = 1
alfa3 = 2**(4/3)
err_new1 = np.abs(np.array(v_xk_n1)-alfa1)
print("ERRORE NEWTOWN:",err_new1)
err_new2 = np.abs(np.array(v_xk_n2)-alfa2)
err_new3 = np.abs(np.array(v_xk_n3)-alfa3)
